{
  "name": "manga-converter",
  "version": "0.0.1",
  "description": "manga converter in dark mode",
  "start": "index.ts",
  "scripts": {
    "start": "node .",
    "training": "npx ts-node -r tsconfig-paths/register --transpiler sucrase/ts-node-plugin training/index.ts",
    "test": "npx ts-node -r tsconfig-paths/register --transpiler sucrase/ts-node-plugin training/test.ts",
    "dev": "npx ts-node -r tsconfig-paths/register --transpiler sucrase/ts-node-plugin src/index.ts",
    "build": "tsc && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "lint": "eslint --ext .tsx,.ts --fix ./src -c .eslintrc.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ashu11-A/Manga-Converter.git"
  },
  "author": "Ashu11-A",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ashu11-A/Manga-Converter/issues"
  },
  "homepage": "https://github.com/Ashu11-A/Manga-Converter#readme",
  "dependencies": {
    "@img/sharp-win32-x64": "^0.33.1",
    "@tensorflow-models/deeplab": "^0.2.2",
    "@tensorflow/tfjs-node": "4.15.0",
    "@tensorflow/tfjs-node-gpu": "^4.15.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-http-proxy": "^2.0.0",
    "fs": "^0.0.1-security",
    "jimp": "^0.22.10",
    "node-pre-gyp": "^0.17.0",
    "pngjs": "^7.0.0",
    "sharp": "^0.33.1",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-http-proxy": "^1.6.6",
    "@types/pngjs": "^6.0.4",
    "sucrase": "^3.34.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
